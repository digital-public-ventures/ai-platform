{
	"compilerOptions": {
		"target": "es5",
		"lib": ["es5", "dom"],
		"module": "esnext",
		"moduleResolution": "node",
		"types": ["cypress", "node"],
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"inlineSourceMap": true,
		"sourceMap": false
	},
	"include": ["**/*.ts"]
}
